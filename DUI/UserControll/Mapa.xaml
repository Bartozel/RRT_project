<UserControl x:Class="DUI.Mapa"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Canvas x:Name="canvasMap" Grid.Column="1" Background="WhiteSmoke">
            <Button Name="btnStart" Content="Start" Canvas.Left="10" Canvas.Top="10" Height="20" Width="40" Click="Button_Click_Start"/>
            <Button Name="btnGoal" Content="Goal" Canvas.Left="55" Canvas.Top="10" Height="20" Width="40" Click="Button_Click_Goal"/>
            <Button Name="btnObs" Content="Obs" Canvas.Left="100" Canvas.Top="10" Height="20" Width="40" Click="Button_Click_Obs"/>
            <Button Name="btnDel" Content="Del" Canvas.Left="750" Canvas.Top="10" Height="20" Width="40" Click="Button_Click_Del"/>
        </Canvas>
    </Grid>
</UserControl>
